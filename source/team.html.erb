<div class="wrapper-background">
  <div class="team-content">
    <h1>This template was built with <i class="fa fa-heart"></i> by</h1>
    <div class="team-info">
      <% data.users.each do |teammate| %>
      <div class="team">
        <%= image_tag "https://kitt.lewagon.com/placeholder/users/#{teammate.web_name}", class: "img-circle member"%>
        <h3><%= teammate.name %></h3>
        <% link_to "https://kitt.lewagon.com/alumni/#{teammate.web_name}" do %>
          <i class="fa fa-github"><%= teammate.name %></i>
        <% end %>
      </div>
      <%end%>
    </div>
  </div>
</div>



